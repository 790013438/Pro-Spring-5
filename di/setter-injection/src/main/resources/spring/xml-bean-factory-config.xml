<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="oracle" name="wiseworm" class="io.github.floyd.di.BookwormOracle" scope="prototype"/>

    <bean id="person" class="io.github.floyd.abstra.Person" abstract="true">
        <property name="bookwormOracle">
            <ref bean="oracle"/>
        </property>
        <property name="b">
            <value>10</value>
        </property>
    </bean>

    <bean id="person0x0" class="io.github.floyd.abstra.Person" abstract="true">
        <property name="bookwormOracle">
            <ref bean="oracle"/>
        </property>
        <property name="b">
            <value>16</value>
        </property>
        <property name="paths">
            <ref bean="paths"/>
        </property>
    </bean>

    <bean id="worker" class="io.github.floyd.abstra.Worker" parent="person">
        <property name="paths">
            <ref bean="paths"/>
        </property>
    </bean>

    <bean id="worker0x0" class="io.github.floyd.abstra.Worker" parent="person0x0">
    </bean>

    <bean id="paths" class="java.util.ArrayList">
        <constructor-arg>
            <list>
                <value>a</value>
                <value>b</value>
            </list>
        </constructor-arg>
    </bean>

    <bean id="abstract" abstract="true">
        <property name="bookwormOracle">
            <ref bean="oracle"/>
        </property>
        <property name="b">
            <value>9999</value>
        </property>
        <property name="paths">
            <ref bean="paths"/>
        </property>
    </bean>

    <bean id="worker0x1" class="io.github.floyd.abstra.Worker" parent="abstract">

    </bean>
</beans>
